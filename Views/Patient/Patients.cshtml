@model List<patient>

<table>
    <thead>
        <tr>
            <th>
                Patient ID
            </th>
            <th>
                Patient Name
            </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++) {
        <tr>
            <td>
                <a asp-controller="Patient" asp-action="PatientDetails" asp-route-id="@Model[i].PatientID">@Model[i].PatientID</a> 
            </td>
            <td>
                @Model[i].Name
            </td>
        </tr>
        }
    </tbody>
</table>